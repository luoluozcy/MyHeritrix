(function() {
   var url = location.toString(), idx = url.indexOf('#!'), reg = /^[/]+/;
   url = url.substr(idx + 2);
   var result = url.match(reg), p = (idx != -1 && result && result[0].length == 1) ? 1 : 0;
   p && location.replace(url);
   if(this.parent != this) {this.parent.location = this.location}
})();
</script>
