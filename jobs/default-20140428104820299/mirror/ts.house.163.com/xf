function hunhe(val){
//贷款类别
var i=1;
	if ( val==3 ){
	$(d_jisuan_f1).addCss("display:none");
	$(d_jisuan_f2).addCss("display:block");
		$(d_lilv_show_1).addCss("display:block");
		$(d_lilv_show_2).addCss("display:block");	
		$(dkze).disabled = true;
		jsfs(i,2);
	}
	if ( val==2 ){
	$(d_jisuan_f1).addCss("display:block");
	$(d_jisuan_f2).addCss("display:none");
		$(d_lilv_show_1).addCss("display:none");
		$(d_lilv_show_2).addCss("display:block");	
		$(dkze).disabled = false;
	}
	if ( val==1) {
	$(d_jisuan_f1).addCss("display:block");
	$(d_jisuan_f2).addCss("display:none");
	$(d_lilv_show_1).addCss("display:block");
	$(d_lilv_show_2).addCss("display:none");
	$(dkze).disabled = false;
	if($(dkze).value == '不需填写'){
	$(dkze).value = '';
	}
	}
}
function jsfs(i,t){
//计算方式
	if ( t == 1 && jisuan_dj.checked ){
		$(jisuan_dj_list).addCss("display:block");
		$(jisuan_tt_list).addCss("display:block");
		if( $(type).value == 3 ){
			$(type).value=1;
			hunhe(1);
		}
	}else if( jisuan_tt.checked ){
		$(jisuan_dj_list).addCss("display:block");
		$(jisuan_tt_list).addCss("display:block");
	}
}
//验证是否为数字
function reg_Num(str){
	if (str.length==0){return false;}
	var Letters = "1234567890.";
	for (i=0;i<str.length;i++){
		var CheckChar = str.charAt(i);
		if (Letters.indexOf(CheckChar) == -1){return false;}
	}
	return true;
}
//验证是否为整数
function reg_NFloat(str){
	if (str.length==0){return false;}
	var Letters = "1234567890";
	for (i=0;i<str.length;i++){
		var CheckChar = str.charAt(i);
		if (Letters.indexOf(CheckChar) == -1){return false;}
	}
	return true;
}
//得到利率
function getlilv(lilv_class,type,years){
	var lilv_class = parseInt(lilv_class);
    if (years<=5){
		 return lilv_array[lilv_class][type][5];
	}else{
		return lilv_array[lilv_class][type][10];
	}
}
//本金还款的月还款额(参数: 年利率 / 贷款总额 / 贷款总月份 / 贷款当前月0～length-1)
function getMonthMoney2(lilv,total,month,cur_month){
	var lilv_month = lilv / 12;//月利率
	//return total * lilv_month * Math.pow(1 + lilv_month, month) / ( Math.pow(1 + lilv_month, month) -1 );
	var benjin_money = total/month;
	return (total - benjin_money * cur_month) * lilv_month + benjin_money;
}
//本息还款的月还款额(参数: 年利率/贷款总额/贷款总月份)
function getMonthMoney1(lilv,total,month){
	var lilv_month = lilv / 12;//月利率
	return total * lilv_month * Math.pow(1 + lilv_month, month) / ( Math.pow(1 + lilv_month, month) -1 );
}
function extTol(i){
	if(i<2){// 本息、本金计算
		var years=NTES("#d_years").value;
		var month = years * 12;
		NTES("#r_years").value = month+"(月)";
		if($(type).value==3){ // 组合贷款
			if (!reg_NFloat($(zuhe_sy).value)||$(zuhe_sy).value==''){alert("组合型贷款请填写正确的商贷比例");$(zuhe_sy).focus();return false;}
			if (!reg_NFloat($(zuhe_gj).value)||$(zuhe_gj).value==''){alert("组合型贷款请填写正确的公积金比例");$(zuhe_gj).focus();return false;}
			var total_sy = parseInt($(zuhe_sy).value)*10000;
			var total_gjj = parseInt($(zuhe_gj).value)*10000;
			var daikuan_total = total_sy + total_gjj;
			NTES("#r_fkze").value = "略";//房款总额
			NTES("#r_sqfk").value = 0;//首期付款
			//贷款总额
			NTES("#r_dkze").value = (total_sy + total_gjj)/10000 + '万元';
			//月还款lilv
			var lilv_sd = getlilv(NTES("#d_lilv").value,1, years);//得到商贷利率
			var lilv_gjj = getlilv(NTES("#d_lilv").value,2, years);//得到公积金利率
			if(i==1){//1.本金还款
				var all_total2 = 0;
				var month_money2 = "";
				for(j=0;j<month;j++) {
					//调用函数计算: 本金月还款额
					huankuan = getMonthMoney2(lilv_sd,total_sy,month,j) + getMonthMoney2(lilv_gjj,total_gjj,month,j);
					all_total2 += huankuan;
					huankuan = Math.round(huankuan*100)/100;
					month_money2 += (j+1) +"月," + huankuan + "(元)\n";
				}
				NTES("#r_month_money_b").value = month_money2;
				//还款总额
				NTES("#r_all_total").value = Math.round(all_total2*100)/100;
				//支付利息款
				NTES("#r_zflx").value = Math.round( (all_total2 - daikuan_total) *100)/100;
			}else{//2.本息还款
				//月均还款
				var month_money1 = getMonthMoney1(lilv_sd,total_sy,month) + getMonthMoney1(lilv_gjj,total_gjj,month);//调用函数计算
				NTES("#r_month_money_a").value = Math.round(month_money1*100)/100 + "(元)";
				//还款总额
				var all_total1 = month_money1 * month;
				NTES("#r_all_total").value = Math.round(all_total1*100)/100;
				//支付利息款
				NTES("#r_zflx").value = Math.round( (all_total1 - daikuan_total) *100)/100;
			}
		}else{//--  商业贷款、公积金贷款
			var lilv = getlilv(NTES("#d_lilv").value,$(type).value,years);//得到利率
			if (NTES("#d_jisuan_dj").checked == true){
				//------------ 根据单价面积计算
				if (!reg_Num($(danjia).value)||$(danjia).value==''){alert("计算方式请输入正确的单价");$(danjia).focus();return false;}
				if (!reg_Num($(mianji).value)||$(mianji).value==''){alert("计算方式请输入正确的面积");$(mianji).focus();return false;}
				//房款总额
				var fangkuan_total = $(danjia).value * $(mianji).value;
				NTES("#r_fkze").value = fangkuan_total;
				//贷款总额
				var daikuan_total = (fangkuan_total) * (NTES("#d_anjie").value/10);
				daikuan_total=Math.floor(daikuan_total/10000);
				NTES("#r_dkze").value = daikuan_total + '万';
				daikuan_total=daikuan_total*10000;
				//首期付款
				var money_first = fangkuan_total - daikuan_total;
				NTES("#r_sqfk").value = money_first;
			}else{
				//------------ 根据贷款总额计算
				if (!reg_NFloat($(dkze).value)||$(dkze).value==''){alert("请填写正确的贷款总额");$(dkze).focus();return false;}
				//房款总额
				NTES("#r_fkze").value = "略";
				//贷款总额
				var daikuan_total = $(dkze).value*10000;
				NTES("#r_dkze").value = $(dkze).value + '万';
				//首期付款
				NTES("#r_sqfk").value = 0;
			}
			if(i==1){//1.本金还款
				//月还款
				var all_total2 = 0;
				var month_money2 = "";
				for(j=0;j<month;j++) {
					//调用函数计算: 本金月还款额
					huankuan = getMonthMoney2(lilv,daikuan_total,month,j);
					all_total2 += huankuan;
					huankuan = Math.round(huankuan*100)/100;
					//fmobj.month_money2.options[j] = new Option( (j+1) +"月," + huankuan + "(元)", huankuan);
					month_money2 += (j+1) +"月," + huankuan + "(元)\n";
				}
				NTES("#r_month_money_b").value = month_money2;
				
				//还款总额
				NTES("#r_all_total").value = Math.round(all_total2*100)/100;
				//支付利息款
				NTES("#r_zflx").value = Math.round( (all_total2 - daikuan_total) *100)/100;
			}else{//2.本息还款
				//月均还款
				var month_money1 = getMonthMoney1(lilv,daikuan_total,month);//调用函数计算
				NTES("#r_month_money_a").value = Math.round(month_money1*100)/100 + "";
				//还款总额
				var all_total1 = month_money1 * month;
				NTES("#r_all_total").value = Math.round(all_total1*100)/100;
				//支付利息款
				NTES("#r_zflx").value = Math.round( (all_total1 - daikuan_total) *100)/100;
			}
		}
//		$(fd_result_div).addCss("display:block");
//		$(fd_submit_div).addCss("display:none");
	}
	return false;
}
for (var i=0;i<type.length;i++){// 贷款类别
	var tmptype=type;
	tmptype.num=i;
	$(tmptype).addEvent(
		"change",
		function () { hunhe(this.value) }.bind(tmptype)
	);
	hunhe($(tmptype).value); // 初始化
}
for (var i=0;i<1;i++){ // 计算方式
	jsfs(i,1);
}
for (var i=0;i<bt2.length;i++){ // 重置按钮
	var tmpbt2=bt2;
	tmpbt2.num=i;
	if(i<2){
		$(tmpbt2).addEvent(
			"click",
			function () { hunhe(0) }.bind(tmpbt2)
		);
	}
}
	NTES("#fdjsq").addEvent(
		"submit",
		function () {	
		var fdtype_1_v = $(fdtype_1).value;
		//extTol(fdtype_1_v);
		if( fdtype_1_v == "0" ){
		$(dk_result_a).addCss("display:block");
		$(dk_result_b).addCss("display:none");
		//document.getElementById('dk_show_type').innerHTML='等额本息';
		extTol("0");
		} else {
		$(dk_result_a).addCss("display:none");
		$(dk_result_b).addCss("display:block");
		//document.getElementById('dk_show_type').innerHTML='等额本金（递减）';
		extTol("1");
		}
 extTol(this.num);}.bind(0)
	);
