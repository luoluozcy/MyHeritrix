<script type="text/javascript">
function isMobil(s) {
  			var patrn = /(^0{0,1}1[3|4|5|6|7|8|9][0-9]{9}$)/;
  			if (!patrn.exec(s)) {
  				return false;
  			}
  			return true;
  		}
function baoming(){
  		var shijian=document.getElementById('form_146_shijian');
		var buyname=document.getElementById('form_146_xingming');
		var buymobile=document.getElementById('form_146_shoujihaoma');
		var renshu=document.getElementById('form_146_renshu');
		var buyloupan=document.getElementById('select6');
		//var buyloupan=select.options[select.selectedIndex].value;
           if (buyloupan.value == '') {
  			alert('请输入楼盘名！');
              buyloupan.select();
              buyloupan.focus();
              return false;
          }else  if (buyname.value == '') {
  			alert('请输入你的姓名！');
              buyname.select();
              buyname.focus();
              return false;
          }else if(buymobile.value == '') {
              alert('请输入你的手机号码！');
              buymobile.select();
              buymobile.focus();
              return false;
          }else if(!isMobil(buymobile.value)){
              alert("请输入正确的手机号码！");
              buymobile.select();
              buymobile.focus();
              return false;
          
          }else{
                        
			var formname=document.getElementById('name');
			var formmobile=document.getElementById('mobile');
			var information=document.getElementById('message');
			var loupan=document.getElementById('pname');
            var formemail=document.getElementById('email');
			formname.value=buyname.value;
			formmobile.value=buymobile.value;
			information.value="人数:"+renshu.value+"    "+"时间:"+shijian.value;
			loupan.value=buyloupan.value;
            formemail.value="tangshanhouse@163.com";
			
			document.groupbuyForm.submit();
          }	
  }
</script>
