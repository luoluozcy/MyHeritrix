//更多查询条件
function showSosoText (t) {
	if (t == "on"){
		$("#show-soso-text").style.display ="block";
		$("#show-soso-1").style.display ="none";
		$("#show-soso-2").style.display ="block";
	}else if (t == "hidden"){
		$("#show-soso-text").style.display ="none";
		$("#show-soso-1").style.display ="block";
		$("#show-soso-2").style.display ="none";
	}
}
