new NTES.ui.Slide($("#cMenu_21 > li"), $("#cCnt_21 > dt"), "on", "mouseover")
new NTES.ui.Slide($("#cMenu_22 > li"), $("#cCnt_22 > ul"), "on", "mouseover")
