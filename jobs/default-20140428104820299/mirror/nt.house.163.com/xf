<!-- 校验表单 -->
<script type="text/javascript">
  	function checkTG(fid,pid,sdivid,hdivid){
		var form = document.getElementById(fid);
		form.action="http://xf.house.163.com/nt/groupbuy!saveGroupBuy.action?productid=" + pid ;
		if(form.elements['name'].value == ''){
			alert('"姓名" 为必填项，请填写完整。');
			form.elements['name'].focus();
			return false;
		}
		var mobile = form.elements['mobile'].value
		if(mobile==""){
			alert('"手机" 为必填项，请填写完整。');
			form.elements['mobile'].focus();
			return false;
		}
		var zzstr = /^1(3|5|8)\d{9}$/;
		 if (mobile != "" && !zzstr.test(mobile))
		 {
			alert('请输入正确的11位手机号码');
			return false;
		  }
			form.submit();
			document.getElementById(hdivid).style.display="none";
			document.getElementById(sdivid).style.display="block";
	};
</script>
