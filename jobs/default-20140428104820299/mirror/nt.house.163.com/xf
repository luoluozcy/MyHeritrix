	houseSearchxf[i]=new newHouseForm(NTES(this),oD,oP,oK,i);
	houseSearchFunEval=houseSearchFunEval+"function setAutocompleteData"+i+"(data){data = (data['list']).split(',');houseSearchxf["+i+"].setData(data);}";
});
