              buyloupan.focus();
              return false;
          }else  if (buyname.value == '') {
  			alert('请输入你的姓名！');
              buyname.select();
              buyname.focus();
              return false;
          }else if(buymobile.value == '') {
              alert('请输入你的手机号码！');
              buymobile.select();
              buymobile.focus();
              return false;
          }else if(!isMobil(buymobile.value)){
              alert("请输入正确的手机号码！");
              buymobile.select();
              buymobile.focus();
              return false;
          
          }else{
                        
			var formname=document.getElementById('name');
			var formmobile=document.getElementById('mobile');
