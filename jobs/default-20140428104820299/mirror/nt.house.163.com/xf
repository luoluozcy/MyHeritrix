//<![CDATA[
// 新房搜索
 newHouseForm=function(fobj,oD,oP,oK,order){
	var formSearch = NTES(fobj);
	var selDistrict = NTES(oD);
	var selPlate = NTES(oP);
	var selKeyword = NTES(oK);
	selKeyword[0].defaultVal = selKeyword[0].value;
	var selAll=NTES('select',fobj);
		var arrDistrict = ['请选择区域','全部区域','开发区','通州区','港闸区','崇川区','海安','如东','如皋','海门','启东'];
	var arrDistrictLen=arrDistrict.length;
	var arrPlate = [
		['请选择板块','通州','永兴','城北','易家桥','小石桥','学田','新城','南大街','城西','城东','城南','海安','如东','如皋','海门','启东']
						,['请选择板块']
				,['请选择板块','通州']
				,['请选择板块','永兴','城北']
				,['请选择板块','易家桥','小石桥','学田','新城','南大街','城西','城东','城南']
				,['请选择板块','海安']
				,['请选择板块','如东']
				,['请选择板块','如皋']
				,['请选择板块','海门']
				,['请选择板块','启东']
			];
	var arrPlateLen=arrPlate.length;
	
	var areaGanged=function(){
		formSearch.addEvent("submit", function(){
			if(selKeyword[0].value == selKeyword[0].defaultVal){
				selKeyword[0].value='';
			}
		});
		// 初始化区域
		district=selDistrict[0];
		NTES(district).length = 0;
		for(var a=0;a<arrDistrictLen;a++){
			if(arrDistrict[a]=='请选择区域'){
				district.options[a] = new Option(arrDistrict[a], '');
			}else{
				district.options[a] = new Option(arrDistrict[a], arrDistrict[a]);
			}
		}
		
		// 初始化板块
		var xfplaces=selPlate[0];
		NTES(xfplaces).length = 0;
		arrPlate0Len=arrPlate[0].length;
		for(var a=0;a<arrPlate0Len;a++){
			if(arrPlate[0][a]=='请选择板块'){
				xfplaces.options[a] = new Option(arrPlate[0][a], '');
			}else{
				xfplaces.options[a] = new Option(arrPlate[0][a], arrPlate[0][a]);
			}
		}
		// 区域切换板块
		$(district).onchange=function(){
			$(xfplaces).length = 0;
			var sel=this.selectedIndex-1;
			if(sel>=0){
				var len=arrPlate[sel].length;
				for(var i=0;i<len;i++){
					if(arrPlate[sel][i]=='请选择板块'){
						$(xfplaces).options[i] = new Option(arrPlate[sel][i], '');
					}else{
						$(xfplaces).options[i] = new Option(arrPlate[sel][i], arrPlate[sel][i]);
					}
				}
			}
		}
		
	}
	
	if( (selDistrict.length>0)&&(selPlate.length>0)){ areaGanged(); }
	
	var autoclpT2='';
	var keyworldchar='';
	var cache=new Array();
	var objt={};
	
	var loaded=false;
	var loadedli={};
	var linow=0;
	var liLen=0;
	var iniHtml=function(){
		clearTimeout(autoclpT2);
		var data=cache["'"+keyworldchar+"'"];
		var len=data.length;
		if(len>0){
			var i=0;var html='';
			while(i<len){
				html=html+'<li>'+data[i]+'</li>';
				i++;
			}
			objt[0].innerHTML=html;
			NTES(objt).addCss("display:block;");
			var li=NTES('li',NTES(objt));
			li.addEvent("mouseover", function(){
				//alert(this.innerHTML);
				NTES(this).addCss('on');
			});
			li.addEvent("mouseout", function(){
				NTES(this).removeCss('on');
			});
			li.addEvent("click", function(){
				NTES(li).removeCss('active');
				NTES(this).addCss('active');
				NTES.dom.eachNode(selAll,function(){this.selectedIndex=0;});
				selKeyword[0].value=this.innerHTML;
				formSearch.submit();
				NTES(objt).addCss("display:none;");
			});
			selKeyword.addEvent("mouseover", function(e){
				clearTimeout(autoclpT2);
				NTES(objt).addCss("display:block;");
			})
			selKeyword.addEvent("mouseout", function(e){
				clearTimeout(autoclpT2);
				autoclpT2=setTimeout(function(){NTES(objt).addCss("display:none;");},300);
			})
			
			objt.addEvent("mouseout", function(e){
				clearTimeout(autoclpT2);
				autoclpT2=setTimeout(function(){NTES(objt).addCss("display:none;");},300);
			})
			objt.addEvent("mouseover", function(e){
				clearTimeout(autoclpT2);
			})
			NTES('li',objt).addEvent("mouseover", function(e){
				clearTimeout(autoclpT2);
			})
		}else{
			NTES(objt).addCss("display:none;");
			selKeyword.removeEvent("mouseover")
		}
	}
	var autoComplete=function(){
		var oNewNode = document.createElement("div");
		replaceNode(oNewNode,selKeyword[0]);
		oNewNode.appendChild(selKeyword[0]);
		var oNewNodeUl = document.createElement("ul");
		oNewNodeUl.className='autocomplete-tips';
		oNewNode.appendChild(oNewNodeUl);
		oNewNode.className='autocomplete';
		
		var url='http://xf.house.163.com/nj/search!list.action?q=';
		var t='';
		objt=NTES('.autocomplete-tips',formSearch);
		
		selKeyword.addEvent("keyup", function(e){
			keyworldchar=this.value;
			//alert(val);
			if(e.keyCode!=38 && e.keyCode!=40){
				if(typeof(cache["'"+keyworldchar+"'"])=='undefined'){
					t = setTimeout(function(){
                        var url2=url+keyworldchar+'&callback=setAutocompleteData'+order;
                        NTES.ajax.importJs(url2, function(){	});
					},300);
				}else{
					iniHtml();
					loaded =true;linow=0;
					loadedli=NTES('li',NTES(objt));
					liLen=loadedli.length;
				}
			}
			if((e.keyCode==38 || e.keyCode==40) && loaded && liLen>0){
				if(linow<0){linow=liLen-1}
				if(linow>=liLen){linow=0}
				NTES(loadedli).removeCss('active');
				NTES(loadedli[linow]).addCss('active');
				selKeyword[0].value=NTES(loadedli[linow]).innerHTML;
				if(e.keyCode==38){//向上
					linow--;
				}
				if(e.keyCode==40){//向上
					linow++;
				}
				NTES.dom.eachNode(selAll,function(){this.selectedIndex=0;});
				NTES(objt).addCss("display:block;");
			}
		});
		selKeyword.addEvent("keydown", function(){
			clearTimeout(t);//alert('cl');
		});
	}
	function setData(data){
		if(data!=''){
			cache["'"+keyworldchar+"'"]=data;
		}else{
			cache["'"+keyworldchar+"'"]=[];
		}
		iniHtml();
		loaded =true;linow=0;
		loadedli=NTES('li',NTES(objt));
		liLen=loadedli.length;
	}
	this.setData=function(data){
		setData(data);
	}
	function replaceNode (newNode, oldNode){
		if(oldNode.replaceNode){
			oldNode.replaceNode(newNode);
		} else if (oldNode.parentNode.replaceChild) {
			oldNode.parentNode.replaceChild(newNode, oldNode);
		}
		return newNode;
	}
	if(selKeyword.length>0){ autoComplete(); }
}
