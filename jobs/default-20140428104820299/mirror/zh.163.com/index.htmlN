        <script> 
            $(document).ready(function () {
                var is_phone = function (phone) {
                    return phone.match(/^13\d{9}$|^14\d{9}$|^15\d{9}$|^18\d{9}$|^16\d{9}$/);
                };
 
                $("#invite").click(function () {
                    var name = $("#name").val(), from = $("#from").val(), to = $("#to").val();
                    if (!name) {
                        alert('姓名不能为空');
                        return false;
                    }
 
                    if (!is_phone(from)) {
                        alert('你的手机号码格式错误');
                        return false;
                    }
 
                    if (!is_phone(to)) {
                        alert('朋友的手机号码格式错误');
                        return false;
                    }
 
                    if (from === to) {
                        alert('不能自己邀请自己');
                        return false;
                    }
 
                    $.getJSON(
                        "http://sms-invite.webapp.163.com/invite/zh/guanwang?callback=?",
                        {'var': name, 'from': from, 'to': to},
                        function (ret) {
                            if (ret.result === 'param_error') {
                                alert('参数错误');
                                return false;
                            }
                            if (ret.result === 'include_keywords') {
                                alert('姓名包含关键字');
                                return false;
                            }
                            if (ret.result === 'product_channel_error') {
                                alert('产品/渠道错误');
                                return false;
                            }
                            if (ret.result === 'ip_send_num_limit') {
                                alert('达到ip发送上限');
                                return false;
                            }
                            if (ret.result === 'sender_term_limit') {
                                alert('达到短期发送者发送上限');
                                return false;
                            }
                            if (ret.result === 'receiver_term_limit') {
                                alert('达到短期接收者接收上限');
                                return false;
                            }
                            if (ret.result === 'total_limit') {
                                alert('达到发送手机发送总数限制');
                                return false;
                            }
                            if (ret.result === 'send_sms_error') {
                                alert('系统错误');
                                return false;
                            }
                            if (ret.result === 'server_error') {
                                alert('系统错误');
                                return false;
                            }
                            if (ret.result === 'ok') {
                                alert('邀请成功');
                                return false;
                            }
                        }
                    )
                });
            });
