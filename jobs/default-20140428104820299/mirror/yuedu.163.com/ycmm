    function cutWords() {
        $('.j-cutted').each(function(){
            var limit = $(this).attr('words')*1 - 1;
            var str = $(this).text();
            if(str.length > limit){
                str = $(this).text().slice(0,limit) + '...';
            }            
            $(this).text(str);
        });
    }
    cutWords();
    tabHover();
    logInfo.init();
    function addCrtItem(active){
        if (active == "yc") {
            $("#J_Header").addClass("g-hd-b");
        } else {
            $("#J_Header").addClass("g-hd-p");
        }
        $("#J_Header .m-mianlist .ycmm").addClass("crt");
