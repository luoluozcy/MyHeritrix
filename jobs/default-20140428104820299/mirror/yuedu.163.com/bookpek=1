    var tabHover =function(){
        var hovers = $('#J_2Tabs .icon');
        hovers.mouseenter(function(){
            hovers.removeClass('crt');
            $(this).addClass('crt');
        });
        hovers.mouseleave(function(){
            hovers.removeClass('crt');
        });
    };
    function cutWords() {
        $('.j-cutted').each(function(){
            var limit = $(this).attr('words')*1 - 1;
            var str = $(this).text();
            if(str.length > limit){
                str = $(this).text().slice(0,limit) + '...';
            }            
            $(this).text(str);
        });
    }
    
    cutWords();
    tabHover();   
    logInfo.init();
    function addCrtItem(active){
        if (active == "tushu") {
            $("#J_Header").addClass("g-hd-g");
        }
        $("#J_Header .m-mianlist .tushu").addClass("crt");
        $("a",".m-mianlist ."+active).addClass("j-crt");
        $(".m-headerbottom ."+active).css("display","").find("li").first().addClass("crt");
         $("a",".m-logo").attr("href","/book");
    }
    addCrtItem("tushu");
})();
