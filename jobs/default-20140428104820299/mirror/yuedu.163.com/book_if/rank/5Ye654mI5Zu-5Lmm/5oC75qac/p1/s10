(function(){
	function autoClose(){
		var t = 15;

		setInterval(function(){
			if(t === 0){
				window.location.href = '/';
			}else{
				t--;
				$('#J_TL').text(t);
			}
		},1000);

	}

	//autoClose();

	logInfo.init();
})();
</script>
