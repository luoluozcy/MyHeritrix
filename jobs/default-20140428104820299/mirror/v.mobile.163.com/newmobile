}).use('gallery-paginator', function (Y) {
var rankList01 = Y.one('.rankList01');
var rankList02 = Y.one('.rankList02');
function paging(rowsPerPage, content) {	
	if (content.all('li').size() < rowsPerPage + 1) {
		content.one('.page').addClass('hidden');
	};
	var pageObj = new Y.Paginator({
			rowsPerPage : rowsPerPage,
			template : '{PreviousPageLink}{PageLinks}{NextPageLink}',
			previousPageLinkLabel : '&lt;',
			nextPageLinkLabel : '&gt;'
		});
	pageObj.setTotalRecords(content.all('li').size(), true);
	pageObj.on('changeRequest', function (state) {
		this.setPage(state.page, true);
		this.setRowsPerPage(state.rowsPerPage, true);
		content.all('ul').some(function (node) {
			if (!node.hasClass('hidden')) {
				node.addClass('hidden');
			};
			imgLazyLoad(content.all('ul').item(state.page - 1).removeClass('hidden').all('img'));
		});
	});
	pageObj.render(content.one('.page'));
};
function imgLazyLoad(imgs) {
	imgs.some(function (node, i) {
		var dataSrc = node.getAttribute('data-src');
		if (dataSrc) {
			node.setAttribute('src', dataSrc);
			node.removeAttribute('data-src');
		} else {
			return true;
		}
	});
};
paging(12, rankList01);
paging(12, rankList02);
})
new NTES.ui.Slide ($("#cateBox a"),$("#tab-hd div.rankList"),"on","mouseover");
</script>
