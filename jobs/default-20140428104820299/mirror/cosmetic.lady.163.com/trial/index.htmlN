    //试用列表
    var TrialDataView = function(config) {
      var t = this;
      t.config = config;
      t._init();
    }
    
    TrialDataView.prototype = {
      _init: function() {
        var t = this;
        t.hasPage = false;
        t.content = Y.one(t.config.content);
        t.paginator = Y.one(t.config.paginator);
        t.top = Y.one('#js_tryListTabs');
        //分类
        t.categorys = Y.all(t.config.categoryBox + ' a');
        t.categorys && t.categorys.on('click', function(e){
          e.preventDefault();
          t.config.category = e.target.getAttribute('category');
          t.paginator.empty();
          t.hasPage = false;
          t.loadContent(1);
          t.categorys.removeClass('fB');
          e.target.addClass('fB');
        });
        
        
       //马上申请按钮
        t.content.on('click', function(e){
          var node = e.target;
          TrialListener(e);
		  if(node.hasClass('try-btn-ordered')){e.preventDefault();}
          if(node.hasAttribute('loginurl')){
            e.preventDefault() || e.stopPropagation();
            var lginInfo = getLoginInfo(Y),
                userId = lginInfo.userId,
                isLogin = lginInfo.isLogin;
			if (!isLogin) {
				showLoginBox(Y, node, userId);
			} else {
				if(node.hasClass('try-btn-ordernow')){
					var productiralId = node.getAttribute('producttrialid');
					var orderWrap = node.ancestor('.try-dataBox-order');
					t._orderNow(orderWrap,productiralId);
				}
				else{
					var productiralId = node.getAttribute('producttrialid');
					var orderWrap = node.ancestor('.try-dataBox-order')
					t._cancelOrder(orderWrap,productiralId);
				}
			}
            
          }
        });
        
        t.loadContent(1);
      },
	  initOrderState : function(){
		  var t = this;
		  var lginInfo = getLoginInfo(Y),
              userId = lginInfo.userId,
              isLogin = lginInfo.isLogin;
		  if (isLogin) {
			Y.all('.try-dataBox-order').each(function(){
				var orderWrap = this;
				jQuery.ajax({
					url : '/trial/isSubscribe.do?productTrialId=' + orderWrap.one('.try-btn-order').getAttribute('producttrialid'),
					type : "GET",
					dataType : 'json',
					timeout : 5000,
					error : function() {
						cosmetic.alert("抱歉，服务器出现错误，请重试！");
					},
					success : function(data) {
						if (data.result) {
							var html = [], productiralId = orderWrap.one('.try-btn-order').getAttribute('producttrialid');						
							html.push('<p class="ordertips">订阅成功后，此款宝贝的最新消息将以私信的方式发到您的网易微博账户中。</p><a href="/trial/'+productiralId+'.html" target="_self" class="try-btn-order try-btn-ordered" producttrialid="'+productiralId+'">订阅</a><p class="cancelorder-wrap clearfix"><a href="#" class="cancelorder cDRed" loginurl="http://cosmetic.lady.163.com/trial/" producttrialid="'+productiralId+'">取消订阅&gt;&gt;</a></p>');
							orderWrap.setContent(Y.substitute(html.join(''), data));
						}else{
							var html = [], productiralId = orderWrap.one('.try-btn-order').getAttribute('producttrialid');						
							html.push('<p class="ordertips">订阅成功后，此款宝贝的最新消息将以私信的方式发到您的网易微博账户中。</p><a href="/trial/'+productiralId+'.html" loginurl="http://cosmetic.lady.163.com/trial/" target="_self" class="try-btn-order try-btn-ordernow" producttrialid="'+productiralId+'">订阅</a><p class="cancelorder-wrap clearfix"></p>');
							orderWrap.setContent(Y.substitute(html.join(''), data));
						}
					}
				});
				
			});
		  }
	  },
	  _orderNow : function(orderWrap,productiralId){
		jQuery.ajax({
			url : '/trial/subscribe.do?productTrialId=' + productiralId ,
			type : "GET",
			dataType : 'json',
			timeout : 5000,
			error : function() {
				cosmetic.alert("抱歉，服务器出现错误，请重试！");
			},
			success : function(data) {
				if (data.result) {
					var html = [];
					html.push('<p class="ordertips">订阅成功后，此款宝贝的最新消息将以私信的方式发到您的网易微博账户中。</p><a href="/trial/'+productiralId+'.html" target="_self" class="try-btn-order try-btn-ordered" producttrialid="'+productiralId+'">订阅</a><p class="cancelorder-wrap clearfix"><a href="#" class="cancelorder cDRed" loginurl="http://cosmetic.lady.163.com/trial/" producttrialid="'+productiralId+'">取消订阅&gt;&gt;</a></p>');
					orderWrap.setContent(Y.substitute(html.join(''), data));
				}
				else{
					cosmetic.alert("抱歉，订阅失败，请重试！");
				}
			}
		});
	  },
	  _cancelOrder : function(orderWrap,productiralId){
		jQuery.ajax({
			url : '/trial/unsubscribe.do?productTrialId=' + productiralId ,
			type : "GET",
			dataType : 'json',
			timeout : 5000,
			error : function() {
				cosmetic.alert("抱歉，服务器出现错误，请重试！");
			},
			success : function(data) {
				if (data.result) {
					var html = [];
					html.push('<p class="ordertips">订阅成功后，此款宝贝的最新消息将以私信的方式发到您的网易微博账户中。</p><a href="/trial/'+productiralId+'.html" loginurl="http://cosmetic.lady.163.com/trial/" target="_self" class="try-btn-order try-btn-ordernow" producttrialid="'+productiralId+'">订阅</a><p class="cancelorder-wrap clearfix"></p>');
					orderWrap.setContent(Y.substitute(html.join(''), data));
					cosmetic.alert("已成功取消订阅！");
				}
				else{
					cosmetic.alert("抱歉，取消订阅失败，请重试！");
				}
			}
		});
	  },
      initPage: function() {
        var t = this;
        var p = new Y.Paginator({
          pageReportTemplate: '{totalPages}',
          totalRecords: t.config.totalRecords,
          rowsPerPage: t.config.rowsPerPage,
          firstPageLinkLabel:    '首页',
          previousPageLinkLabel: '上一页',
          nextPageLinkLabel:     '下一页',
          lastPageLinkLabel:     '最后页',
          template: '{FirstPageLink} {PreviousPageLink}  {PageLinks} {NextPageLink} {LastPageLink}'
        });
        p.on('changeRequest', function(state) {
          this.setPage(state.page, true);
          this.setRowsPerPage(state.rowsPerPage, true);
          t.loadContent(state.page);
          t.top.scrollIntoView(true);
        });
        p.render(t.config.paginator);
        p.setPage(1, true);   
        t.hasPage = true;
      },
      loadContent: function(gotoPage) {
        var t = this;
        var cfg = {
          method: 'GET',
          data: 'page=' + gotoPage + '&size=' + t.config.rowsPerPage + '&category=' + t.config.category + '&type=' + t.config.type
,
          on: {
            success: Y.bind(t._handleSuccess, this, gotoPage),
            failure: Y.bind(t._handleFailure, this)
          }
        }
        var request = Y.io(t.config.api, cfg);
      },
      _handleSuccess: function(page, ioId, o) {
        var t = this;
        var json = Y.JSON.parse(o.responseText);
        
        t.config.totalRecords = json.data.total;
        if (json.data.total === 0) {
          t.content.setContent(t.config.CONTENT_NODATA_TEMPLATE);
          return false;
        }
        if (!t.hasPage) {
          t.initPage();
        }
          
        t.content.setContent(json.data.content);
        
        //分享
        var contentShares = t.content.all('.try-dataBox');
        contentShares.each(function(shareContent, i){
          var trialID = shareContent.get('id'),srcnodes;
		  if(shareContent.hasClass('convert-dataBox')){
			  srcnodes = '#shareArea_convert' + trialID;
		  }else{
			  srcnodes = '#shareArea_trial' + trialID;
		  }
          new ShareBtnsWidget({
            srcNode: srcnodes,
            url: 'http://' + document.domain + '/trial/' + trialID + '.html',  
            title: '鸡冻！我在网易女人免费试用了【' + Y.one('#brand_' + trialID).getContent() + '】' + Y.one('#product_' + trialID).getContent() + '_网易女人化妆品库',
            pic: [Y.one('#img_' + trialID).get('src') , 'http://img1.cache.netease.com/lady/img09/index091203/logo.gif']
          });
        });
        
        //倒计时
        var countdowns = t.content.all('[remaining]');
        countdowns && countdowns.each(function(countdown, i){
          if(countdown.getAttribute('id').split('_')[0] == 'countdownBox'){
			var config_b = {'dayFlag':true,'hourFlag':true,'minuteFlag':true,refreshtime:60000}
          	new CountdownWidget(countdown,config_b);
		  }else if(countdown.getAttribute('id').split('_')[0] == 'countdownBox2'){
			var config_c = {'dayFlag':false,'hourFlag':true,'minuteFlag':true,'secondFlag':true,refreshtime:1000}
          	new CountdownWidget(countdown,config_c);
		  }
        });
		
		if(t.config.ifRequest){ t.initOrderState(); }
      
      },
      _handleFailure: function() {
        var t = this;
        t.content.setContent('<div class="mt-20">加载失败...</div>');
      }
    }
    
