        }
    }
  }).use('ntes-tabs-base', 'substitute', 'transition', 'gallery-paginator', 'io-base', 'json-parse', 'event-mouseenter', 'event-resize', 'anim', 'overlay', 'cookie', function(Y) {
    
    var tabs1 = new Y.Tabs({ srcNode: ('#js_tryListTabs'), triggers: '.js_tryListTrigger', panels: '.js_tryListPanel', triggerEvent: 'click', render: true });
    if (window.location.hash == '#tryListPanel2'){
      tabs1.select(2);
    }
    
    //幸运用户滚动
    Y.all('.js_marqueeBox').each(function(marqueeBox){
      var content = marqueeBox.one('ul'),
          copy = Y.Node.create('<ul></ul>'),
        marqueeInner = marqueeBox.one('> div'),
        marqueeInnerDOM = Y.Node.getDOMNode(marqueeInner),
        SCROLL_HEIGHT = content.get('scrollHeight'),
        MOVE_DIST = 105,
        MOVE_INTERVAL = 5000;
        
      if(SCROLL_HEIGHT > 105) {
        //设置初始样式
        marqueeBox.setStyle('height', '105px').setStyle('overflow', 'hidden').setStyle('position', 'relative');
        marqueeInner.setStyle('position', 'absolute').setStyle('top', '0px');
        //复制节点
        copy.addClass('try-lucky-list clearfix');
        copy.setContent(content.getContent());
        marqueeInner.append(copy);
        

        var anim = new Y.Anim({
            node: marqueeInner,
            to: {
              top: function(node){
                return parseInt(node.getStyle('top')) - MOVE_DIST + 'px';
              }
            },
            duration: 0.5
          }),
          intervalFunc = function(){
            anim.run();
          },
          interval;
        
        anim.on("end", function(){
          var top = parseInt(marqueeInner.getStyle('top'));
          if(top + SCROLL_HEIGHT <= 0){  //上面的ul已经移出可见范围 改变top并且换到下面
            marqueeInnerDOM.appendChild(marqueeInnerDOM.firstChild);
            marqueeInner.setStyle('top', top + SCROLL_HEIGHT + 'px');
          }
        });
        
        interval = setInterval(function(){
          intervalFunc();
        }, MOVE_INTERVAL);
        
        marqueeBox.on('mouseenter', function(){
          clearInterval(interval);
        });
        marqueeBox.on('mouseleave', function(){
          interval = setInterval(function(){
            intervalFunc();
          }, MOVE_INTERVAL);
        });
      }
    });
    
    
    //倒计时
    var CountdownWidget = function(srcNode,config) {
      var t = this;
      t.srcNode = srcNode;
	  t.config = config;
      t._init();
    }
    
    CountdownWidget.prototype = {
      _init : function(){
        var t = this;
        if(t.config.dayFlag == true){t.dayArea = t.srcNode.one('.js_timeDay');}
        if(t.config.hourFlag == true){t.hourArea = t.srcNode.one('.js_timeHour');}
        if(t.config.minuteFlag == true){t.minuteArea = t.srcNode.one('.js_timeMinute');}
		if(t.config.secondFlag == true){t.secondArea = t.srcNode.one('.js_timeSecond');}
        t.remainingTime = parseInt(t.srcNode.getAttribute('remaining'));
		t.remaining = {day : 0, hour : 0, minute : 0, second : 0};
		if(t.remainingTime<0){
			t.setTimeContent();
		}else{
			t.getTimeContent();
			t.setTimeContent();
			t._interval = window.setInterval(function(){
			 	t._intervalFun();
			}, t.config.refreshtime); 
		}       
      },
      _intervalFun : function() {
        var t = this;
        t.remainingTime -= t.config.refreshtime;
        if(t.remainingTime > 0){
          t.getTimeContent();  
          t.setTimeContent();
        } else {
          t.remaining = {day : 0, hour : 0, minute : 0, second : 0}
          t.setTimeContent();
          clearInterval(t._interval);
        }
      },
      setTimeContent : function() {
        var t = this;
        if(t.config.dayFlag == true){t.dayArea.setContent(t.remaining.day)};
        if(t.config.hourFlag == true){t.hourArea.setContent(t.remaining.hour)};
        if(t.config.minuteFlag == true){t.minuteArea.setContent(t.remaining.minute)};
		if(t.config.secondFlag == true){t.secondArea.setContent(t.remaining.second)};
      },
      getTimeContent : function(){
        var t = this;
        t.remaining.day = Math.floor( t.remainingTime / ( 24 * 60 * 60 * 1000 ) );
        t.remaining.hour = Math.floor( t.remainingTime / ( 60 * 60 * 1000 ) ) - t.remaining.day * 24 ;
        t.remaining.minute = Math.floor( t.remainingTime / ( 60 * 1000 ) ) - t.remaining.day * 24 * 60 - t.remaining.hour * 60 ;
		t.remaining.second = Math.floor( t.remainingTime / 1000 ) - t.remaining.day * 24 * 60 * 60 - t.remaining.hour * 60 * 60 - t.remaining.minute * 60;
      }
    }
     
    //顶部试用剩余时间刷新
    var topCountdownBox = Y.one('#countdownBox_10208'),
		attentionNum = Y.one('#attentionnum'),
		applyedNum = Y.one('#applyednum'),
		convertrmNum = Y.one('#convertrmnum'),
		remiancopyNum = Y.one('#remiancopynum'),
		remiancopyPercent = Y.one('#remiancopy_percent');
		
    Y.io('/trial/remain.json', {
      methos: 'GET',
      data: 'productTrialId=10208',
      on: {
        success: function(ioId, o){
          var json = Y.JSON.parse(o.responseText);
          topCountdownBox.setAttribute('remaining', json.data.remaining);
          attentionNum.setContent(json.data.attentionCount);
          applyedNum.setContent(json.data.applyCount);
          if(typeof(convertrmNum)!='undefined' && convertrmNum != null){
            convertrmNum.setContent(json.data.exchangeRemainCount);
          }
          remiancopyNum.setContent(json.data.applyed);
          remiancopyPercent.setStyle('width',(json.data.applyed/json.data.amount)*100+'%');
