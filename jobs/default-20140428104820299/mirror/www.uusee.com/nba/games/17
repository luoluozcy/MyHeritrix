<script>
function gotoSearch(){
	if(true){window.location.href = 'http://www.uusee.com/';return false;}
	var keywd = encodeURIComponent(jQuery("#key").val());
	if("" == keywd){
		alert("搜索内容不能为空！");
		return false;
	}else if("请输入关键字" == jQuery("#key").val()){return false;}
	window.location.href = 'http://home.uusee.com/search/result/?key='+keywd;
	return true;
}
</script>
