    <script type="text/javascript">
        var flagSearchKeyword = false;
        var flagSearchRefNo = false;
        var txtSearchKeyword = "请输入您需要查找的内容或关键字";
        function searchcontrol_keyword_txtonfocus(control) {
            if (!flagSearchKeyword) { control.value = ""; control.style.color = "#999"; }
        }
        function searchcontrol_keyword_txtonblur(control) {
            if (control.value != "") { flagSearchKeyword = true; } else { flagSearchKeyword = false; }
            if (!flagSearchKeyword) { control.value = txtSearchKeyword; control.style.color = "#999"; }
        }
        function GoToSearch() {
            var key = document.getElementById("txtSearchKeys").value;
            var strKey = "请输入关键字(楼盘名或路段名)";
            var url;
            if (strKey.indexOf(key) >= 0 || key.length == 0) {
                url = "http://gz.centanet.com/sale/gz/";
            }
            else {
                url = "http://gz.centanet.com/sale/gz/kw" + encodeURI(key) + "/";
            }
            window.location = url;
        }
        $(document).ready(function () {
            $("#txtSearchKeys").autocomplete({
                source: function (request, response) {
                    $.ajax({
                        url: "/AutoComplete.ashx",
                        dataType: "json",
                        data: {
                            k: $("#txtSearchKey").val()
                        },
                        success: function (data) {
                            response($.map(data.props, function (item) {
                                return {
                                    name: item.name,
                                    count: item.count,
                                    value: item.name
                                }
                            }));
                        }
                    });
                },
                minLength: 1,
                select: {}
            })
    .data('autocomplete')._renderItem = function (ul, item) {
        return $('<li></li>').data('item.autocomplete', item)
                             .append('<a  style="height:24px;line-height:24px;width:330px;font-size:13px;"><span style="float:left">' + item.name + '</span><span style="float:right"></span></a>')
                             .appendTo(ul); alert('item' + item); alert('ul' + ul);
    };
        });

        $(function () {
            $("#theArea").click(function () {
                $("#clmarea1").show();
            });
            $("#clmarea").bind("mouseleave", function () {
                $("#clmarea1").hide();
            });
        });
    </script>
    <script type="text/JavaScript">
        function GetTxtValue(id) {
            var value;
            value = document.getElementById(id).value;
            return value;
        }

        function GetIntValue(id) {
            var value;
            value = document.getElementById(id).value;
            if (value == "") {
                return 0;
            }

            if (isNaN(value)) {
                return 0;
            }
            else {
                return value;
            }
        }

        function GetSearchKey() {
            var key = document.getElementById("txtSearchKey").value;
            var strKey = "请输入关键字(楼盘名或路段名)";
            if (strKey.indexOf(key) >= 0 || key.length == 0) {
                return "";
            }
            else {
                return encodeURI(key);
            }
        }

        function SetStyle(id) {

            document.getElementById(id).style.backgroundColor = "#CCCCCC";
        }
        function SetStyleOut(id) {
            document.getElementById(id).style.backgroundColor = "#666666";
        }
        
    </script>
