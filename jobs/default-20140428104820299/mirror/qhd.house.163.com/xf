function showlilv(){
	var years = NTES("#d_years").value
	var lilv = NTES("#d_lilv").value
	if (years<=5) {
	var yearsT = 5;
	}
	else {
	var yearsT = 10;
	}
	var lilv_sd_show = accMul(getlilv(NTES("#d_lilv").value,1, yearsT),100);//µÃµ½ÉÌ´ûÀûÂÊ
	var lilv_gjj_show = accMul(getlilv(NTES("#d_lilv").value,2, yearsT),100);//µÃµ½¹«»ý½ðÀûÂÊ
	document.getElementById('lilv_sd_show').innerHTML=lilv_sd_show+'%';
	document.getElementById('lilv_gjj_show').innerHTML=lilv_gjj_show+'%';
}
function accMul(arg1,arg2){ 
    var m=0,s1=arg1.toString(),s2=arg2.toString(); 
    try{m+=s1.split(".")[1].length}catch(e){} 
    try{m+=s2.split(".")[1].length}catch(e){} 
    return Number(s1.replace(".",""))*Number(s2.replace(".",""))/Math.pow(10,m) 
}
