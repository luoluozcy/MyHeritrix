function hunhe(val){
//´û¿îÀà±ð
var i=1;
	if ( val==3 ){
	$(d_jisuan_f1).addCss("display:none");
	$(d_jisuan_f2).addCss("display:block");
		$(d_lilv_show_1).addCss("display:block");
		$(d_lilv_show_2).addCss("display:block");	
		$(dkze).disabled = true;
		jsfs(i,2);
	}
	if ( val==2 ){
	$(d_jisuan_f1).addCss("display:block");
	$(d_jisuan_f2).addCss("display:none");
		$(d_lilv_show_1).addCss("display:none");
		$(d_lilv_show_2).addCss("display:block");	
		$(dkze).disabled = false;
	}
	if ( val==1) {
	$(d_jisuan_f1).addCss("display:block");
	$(d_jisuan_f2).addCss("display:none");
	$(d_lilv_show_1).addCss("display:block");
	$(d_lilv_show_2).addCss("display:none");
	$(dkze).disabled = false;
	if($(dkze).value == '²»ÐèÌîÐ´'){
	$(dkze).value = '';
	}
	}
}
function jsfs(i,t){
//¼ÆËã·½Ê½
	if ( t == 1 && jisuan_dj.checked ){
		$(jisuan_dj_list).addCss("display:block");
		$(jisuan_tt_list).addCss("display:block");
		if( $(type).value == 3 ){
			$(type).value=1;
			hunhe(1);
		}
	}else if( jisuan_tt.checked ){
		$(jisuan_dj_list).addCss("display:block");
		$(jisuan_tt_list).addCss("display:block");
	}
}
//ÑéÖ¤ÊÇ·ñÎªÊý×Ö
function reg_Num(str){
	if (str.length==0){return false;}
	var Letters = "1234567890.";
	for (i=0;i<str.length;i++){
		var CheckChar = str.charAt(i);
		if (Letters.indexOf(CheckChar) == -1){return false;}
	}
	return true;
}
//ÑéÖ¤ÊÇ·ñÎªÕûÊý
function reg_NFloat(str){
	if (str.length==0){return false;}
	var Letters = "1234567890";
	for (i=0;i<str.length;i++){
		var CheckChar = str.charAt(i);
		if (Letters.indexOf(CheckChar) == -1){return false;}
	}
	return true;
}
//µÃµ½ÀûÂÊ
function getlilv(lilv_class,type,years){
	var lilv_class = parseInt(lilv_class);
    if (years<=5){
		 return lilv_array[lilv_class][type][5];
	}else{
		return lilv_array[lilv_class][type][10];
	}
}
//±¾½ð»¹¿îµÄÔÂ»¹¿î¶î(²ÎÊý: ÄêÀûÂÊ / ´û¿î×Ü¶î / ´û¿î×ÜÔÂ·Ý / ´û¿îµ±Ç°ÔÂ0¡«length-1)
function getMonthMoney2(lilv,total,month,cur_month){
	var lilv_month = lilv / 12;//ÔÂÀûÂÊ
	//return total * lilv_month * Math.pow(1 + lilv_month, month) / ( Math.pow(1 + lilv_month, month) -1 );
	var benjin_money = total/month;
	return (total - benjin_money * cur_month) * lilv_month + benjin_money;
}
//±¾Ï¢»¹¿îµÄÔÂ»¹¿î¶î(²ÎÊý: ÄêÀûÂÊ/´û¿î×Ü¶î/´û¿î×ÜÔÂ·Ý)
function getMonthMoney1(lilv,total,month){
	var lilv_month = lilv / 12;//ÔÂÀûÂÊ
	return total * lilv_month * Math.pow(1 + lilv_month, month) / ( Math.pow(1 + lilv_month, month) -1 );
}
function extTol(i){
	if(i<2){// ±¾Ï¢¡¢±¾½ð¼ÆËã
		var years=NTES("#d_years").value;
		var month = years * 12;
		NTES("#r_years").value = month+"(ÔÂ)";
		if($(type).value==3){ // ×éºÏ´û¿î
			if (!reg_NFloat($(zuhe_sy).value)||$(zuhe_sy).value==''){alert("×éºÏÐÍ´û¿îÇëÌîÐ´ÕýÈ·µÄÉÌ´û±ÈÀý");$(zuhe_sy).focus();return false;}
			if (!reg_NFloat($(zuhe_gj).value)||$(zuhe_gj).value==''){alert("×éºÏÐÍ´û¿îÇëÌîÐ´ÕýÈ·µÄ¹«»ý½ð±ÈÀý");$(zuhe_gj).focus();return false;}
			var total_sy = parseInt($(zuhe_sy).value)*10000;
			var total_gjj = parseInt($(zuhe_gj).value)*10000;
			var daikuan_total = total_sy + total_gjj;
			NTES("#r_fkze").value = "ÂÔ";//·¿¿î×Ü¶î
			NTES("#r_sqfk").value = 0;//Ê×ÆÚ¸¶¿î
			//´û¿î×Ü¶î
			NTES("#r_dkze").value = (total_sy + total_gjj)/10000 + 'ÍòÔª';
			//ÔÂ»¹¿îlilv
			var lilv_sd = getlilv(NTES("#d_lilv").value,1, years);//µÃµ½ÉÌ´ûÀûÂÊ
			var lilv_gjj = getlilv(NTES("#d_lilv").value,2, years);//µÃµ½¹«»ý½ðÀûÂÊ
			if(i==1){//1.±¾½ð»¹¿î
				var all_total2 = 0;
				var month_money2 = "";
				for(j=0;j<month;j++) {
					//µ÷ÓÃº¯Êý¼ÆËã: ±¾½ðÔÂ»¹¿î¶î
					huankuan = getMonthMoney2(lilv_sd,total_sy,month,j) + getMonthMoney2(lilv_gjj,total_gjj,month,j);
					all_total2 += huankuan;
					huankuan = Math.round(huankuan*100)/100;
					month_money2 += (j+1) +"ÔÂ," + huankuan + "(Ôª)\n";
				}
				NTES("#r_month_money_b").value = month_money2;
				//»¹¿î×Ü¶î
				NTES("#r_all_total").value = Math.round(all_total2*100)/100;
				//Ö§¸¶ÀûÏ¢¿î
				NTES("#r_zflx").value = Math.round( (all_total2 - daikuan_total) *100)/100;
			}else{//2.±¾Ï¢»¹¿î
				//ÔÂ¾ù»¹¿î
				var month_money1 = getMonthMoney1(lilv_sd,total_sy,month) + getMonthMoney1(lilv_gjj,total_gjj,month);//µ÷ÓÃº¯Êý¼ÆËã
				NTES("#r_month_money_a").value = Math.round(month_money1*100)/100 + "(Ôª)";
				//»¹¿î×Ü¶î
				var all_total1 = month_money1 * month;
				NTES("#r_all_total").value = Math.round(all_total1*100)/100;
				//Ö§¸¶ÀûÏ¢¿î
				NTES("#r_zflx").value = Math.round( (all_total1 - daikuan_total) *100)/100;
			}
		}else{//--  ÉÌÒµ´û¿î¡¢¹«»ý½ð´û¿î
			var lilv = getlilv(NTES("#d_lilv").value,$(type).value,years);//µÃµ½ÀûÂÊ
			if (NTES("#d_jisuan_dj").checked == true){
				//------------ ¸ù¾Ýµ¥¼ÛÃæ»ý¼ÆËã
				if (!reg_Num($(danjia).value)||$(danjia).value==''){alert("¼ÆËã·½Ê½ÇëÊäÈëÕýÈ·µÄµ¥¼Û");$(danjia).focus();return false;}
				if (!reg_Num($(mianji).value)||$(mianji).value==''){alert("¼ÆËã·½Ê½ÇëÊäÈëÕýÈ·µÄÃæ»ý");$(mianji).focus();return false;}
				//·¿¿î×Ü¶î
				var fangkuan_total = $(danjia).value * $(mianji).value;
				NTES("#r_fkze").value = fangkuan_total;
				//´û¿î×Ü¶î
				var daikuan_total = (fangkuan_total) * (NTES("#d_anjie").value/10);
				daikuan_total=Math.floor(daikuan_total/10000);
				NTES("#r_dkze").value = daikuan_total + 'Íò';
				daikuan_total=daikuan_total*10000;
				//Ê×ÆÚ¸¶¿î
				var money_first = fangkuan_total - daikuan_total;
				NTES("#r_sqfk").value = money_first;
			}else{
				//------------ ¸ù¾Ý´û¿î×Ü¶î¼ÆËã
				if (!reg_NFloat($(dkze).value)||$(dkze).value==''){alert("ÇëÌîÐ´ÕýÈ·µÄ´û¿î×Ü¶î");$(dkze).focus();return false;}
				//·¿¿î×Ü¶î
				NTES("#r_fkze").value = "ÂÔ";
				//´û¿î×Ü¶î
				var daikuan_total = $(dkze).value*10000;
				NTES("#r_dkze").value = $(dkze).value + 'Íò';
				//Ê×ÆÚ¸¶¿î
				NTES("#r_sqfk").value = 0;
			}
			if(i==1){//1.±¾½ð»¹¿î
				//ÔÂ»¹¿î
				var all_total2 = 0;
				var month_money2 = "";
				for(j=0;j<month;j++) {
					//µ÷ÓÃº¯Êý¼ÆËã: ±¾½ðÔÂ»¹¿î¶î
					huankuan = getMonthMoney2(lilv,daikuan_total,month,j);
					all_total2 += huankuan;
					huankuan = Math.round(huankuan*100)/100;
					//fmobj.month_money2.options[j] = new Option( (j+1) +"ÔÂ," + huankuan + "(Ôª)", huankuan);
					month_money2 += (j+1) +"ÔÂ," + huankuan + "(Ôª)\n";
				}
				NTES("#r_month_money_b").value = month_money2;
				
				//»¹¿î×Ü¶î
				NTES("#r_all_total").value = Math.round(all_total2*100)/100;
				//Ö§¸¶ÀûÏ¢¿î
				NTES("#r_zflx").value = Math.round( (all_total2 - daikuan_total) *100)/100;
			}else{//2.±¾Ï¢»¹¿î
				//ÔÂ¾ù»¹¿î
				var month_money1 = getMonthMoney1(lilv,daikuan_total,month);//µ÷ÓÃº¯Êý¼ÆËã
				NTES("#r_month_money_a").value = Math.round(month_money1*100)/100 + "";
				//»¹¿î×Ü¶î
				var all_total1 = month_money1 * month;
				NTES("#r_all_total").value = Math.round(all_total1*100)/100;
				//Ö§¸¶ÀûÏ¢¿î
				NTES("#r_zflx").value = Math.round( (all_total1 - daikuan_total) *100)/100;
			}
		}
//		$(fd_result_div).addCss("display:block");
//		$(fd_submit_div).addCss("display:none");
	}
	return false;
}
