function showZtc (op){
	var ztc_list = [{"title": "","stitle": "","href": "http://jn.house.163.com/13/0913/14/98LK616Q02130570.html"},{"title": "","stitle": "","href": "http://jn.house.163.com/13/0830/16/97HP35V102130570.html"},{"title": "","stitle": "","href": "http://jn.house.163.com/13/0906/05/982M4GK402130570.html"}];
	var str ="";
	for (var i=0; i < ztc_list.length; i++){
		str += '<li onClick="showZtc(\''+ ztc_list[i].title +'\');">'+ ztc_list[i].title + '</li>';
		if (ztc_list[i].title == op) {
			$("#ztc_stitle").innerHTML = ztc_list[i].stitle;
			$("#input7").innerHTML = ztc_list[i].title;
			$("#ztc_luxian").value = ztc_list[i].title;
			$("#ztc_link").href = ztc_list[i].href;
                        $("#ztc_link1").href = ztc_list[i].href;
			
		}
	}
	$("#zl-list7").innerHTML = str;
	$("#zl-list7").style.display ="none";
};
