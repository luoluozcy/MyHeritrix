//论坛搜索入口
function bbsSearchs(){
	this.box='';
	this.tips='';
	this.key='';
}
bbsSearchs.prototype.init = bbsSearchs_init;
bbsSearchs.prototype.smb = bbsSearchs_sm;
bbsSearchs.prototype.s = bbsSearchs_s;
function bbsSearchs_init(id){
	this.box=NTES('#'+id);
	this.tips=NTES('.tips',NTES(this.box));
	this.key=NTES('input.txt',NTES(this.box));
	this.tips.addCss('display:none');
	this.clt='';
	NTES(this.box).addEvent("mouseout", function() {
		clearTimeout(this.clt);
		this.clt = setTimeout(function(){this.tips.addCss('display:none');}.bind(this),800);
	}.bind(this));
	NTES(this.box).addEvent("mouseover", function() {
		clearTimeout(this.clt);
	}.bind(this));
	NTES(this.tips).addEvent("mouseover", function() {
		clearTimeout(this.clt);
	}.bind(this));
	NTES(this.key).addEvent("mouseover", function() {
		if(NTES(this.tips)[0].innerHTML!='' && NTES(this.tips)[0].innerHTML.indexOf('搜索中')<=0 && NTES(this.tips)[0].innerHTML.indexOf('没有结果')<=0){
			NTES(this.tips).removeCss('display:none');
		}
	}.bind(this));
}
function bbsSearchs_sm(){
	NTES(document).removeEvent("click", function() { this.tips.addCss('display:none'); }.bind(this));
	if(NTES(this.key)[0].value!='' && NTES(this.key)[0].value!='请输入关键字'){
		this.s(NTES(this.key)[0].value);
		NTES(this.tips).removeCss('display:none');
		NTES(this.tips)[0].innerHTML='<li>关键字"'+NTES(this.key)[0].value+'"搜索中……</li>';
	}
	return false;
}
function bbsSearchs_s(key){
	NTES.ajax.importJs(
	"http://bbs.gz.house.163.com/bbs/goBoard.jsp?q="+key,
	function() {
		if(typeof(boards)!='undefined'){
		if(boards!=''){
			var boardsL=boards.length//alert(boards[0]);
			var html='';
			for(var i=0;i<boardsL;i++){
				html=html+'<li><a href="'+boards[i][2]+'">'+boards[i][1]+'</a></li>';
			}
			NTES(this.tips)[0].innerHTML=html;
		}else{NTES(this.tips)[0].innerHTML='<li>没有结果，请更换关键字。</li>';}
		}else{
			NTES(this.tips)[0].innerHTML='<li>没有结果，请更换关键字。</li>';
		}boards='';
	}.bind(this),
	"gb2312"
	);
}
