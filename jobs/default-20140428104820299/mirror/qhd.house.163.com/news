					NTES(this).NTES('a.name').removeCss('display:none;');
					NTES(this).NTES('div.cover').removeCss('display:none;');
				}
			}
		},i);
	});
});
function GetClearWaterRemark(obj, text) {
	if (obj.value == text) {
		obj.value = "";
		obj.style.color = "#252525";
	}
}
function WaterRemark(obj, text) {
	if (obj.value == "") {
		obj.value = text;
		obj.style.color = "#666";
	}
}
