function showmsg(){
	var tgpname =NTES("#pname");
	var tgname =NTES("#name");
	var tgmobile =NTES("#mobile");
	if(tgpname.value =="" || tgpname.value =="输入楼盘中文/全拼/缩写"){
		this.alert('请填写预购楼盘');
		tgpname.focus();
		return false;
	}
	if(tgname.value==""){
		this.alert('请填写姓名');
		tgname.focus();
		return false;
	}
	if(tgmobile.value==""){
		this.alert('请填写手机');
		tgmobile.focus();
		return false;
	}
	alert("发表团购信息成功！");
	NTES("#groupbuyForm").submit;
	return true;
}
function clearlpText(){
	var tgpname =NTES("#pname");
	if (tgpname.value == "输入楼盘中文/全拼/缩写")
	{
		tgpname.value = "";
	}
	tgpname.select();
}
function clearfield(){
	var message =NTES("#message");
	if (message.value == "我想购买")
	{
		message.value = "";
	}
	message.select();
}
