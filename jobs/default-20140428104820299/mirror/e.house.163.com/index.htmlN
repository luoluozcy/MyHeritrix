				url=encodeURI(url);
				url=encodeURI(url);
				window.open(url);
				return false;
			});
			// 初始化区域
			district=selDistrict;
			NTES(district).length = 0;
			var districtData = _district;
			district.options[0] = new Option('城区', '0');
			district.options[1] = new Option('全部区域', '0');
			countData = 1;
			for(key in districtData){
				tempVal = districtData[key];
				if(tempVal){
					countData++;
					district.options[countData] = new Option(tempVal, key);
				}
			};
			
			// 初始化板块
			var xfplaces=selPlate;
			NTES(xfplaces).length = 0;
			var arrPlateData = _plate;
			xfplaces.options[0] = new Option('选择板块', '0');
			countData = 0;
			for(key in arrPlateData){
				tempVal = arrPlateData[key];
				for(key2 in tempVal){
					var tempVal2 = tempVal[key2];
					if(tempVal2){
						countData++;
						xfplaces.options[countData] = new Option(tempVal2, key2);
					}
				}
			};
			// 区域切换板块
			district.onchange=function(){
				xfplaces.length = 0;
				xfplaces.options[0] = new Option('选择板块', '0');
				countData = 0;
				var sel = this.options[this.options.selectedIndex].innerHTML;
				if(sel){
					var plate = arrPlateData[sel];
					for(key in plate){
						tempVal = plate[key];
						if(tempVal){
							countData++;
							xfplaces.options[countData] = new Option(tempVal, key);
						}
					}
				}
			}
			
			// 初始化物业类型
			var propertys=selProperty;
			NTES(propertys).length = 0;
			var propertyData = _property;
			propertys.options[0] = new Option('物业', '0');
			countData = 0;
			for(key in propertyData){
				tempVal = propertyData[key];
				if(tempVal){
					countData++;
					propertys.options[countData] = new Option(tempVal, key);
				}
			};
			// 初始化单价
			var prices=selPrice;
			NTES(prices).length = 0;
			var _priceData = _price;
			prices.options[0] = new Option('价位区间', '0');
			countData = 0;
			for(key in _priceData){
				tempVal = _priceData[key];
				if(tempVal){
					countData++;
					prices.options[countData] = new Option(tempVal, key);
