<script type="text/javascript">
var infoZoneSlide = function(o){
    if(o){
        var scrollMain = NTES('ul',o);
        var list = NTES('li',o);
        var totalNum = list.length;
        var totalWidth = (120 + 15) * totalNum;
        var viewWidth = (120 + 15) * 4;
        var playNext = function(){
            var mLeft = parseInt(scrollMain[0].style.marginLeft);
            var lastWidth = totalWidth + mLeft - viewWidth;
            if(lastWidth <= 0){
                scrollMain[0].style.marginLeft = 0+'px';
            }else if(lastWidth < viewWidth){
                scrollMain[0].style.marginLeft = (viewWidth-totalWidth)+'px';
            }else{
                scrollMain[0].style.marginLeft = mLeft-viewWidth+'px';
            }
        }
        var playPrev = function(){
            var mLeft = parseInt(scrollMain[0].style.marginLeft);
            var lastWidth = totalWidth + mLeft - viewWidth;
            if(totalWidth<viewWidth){
            }else if(mLeft >= 0){
                scrollMain[0].style.marginLeft = 0-lastWidth+'px';
            }else if(0-mLeft <= viewWidth){
                scrollMain[0].style.marginLeft = 0+'px';
            }else{
                scrollMain[0].style.marginLeft = mLeft+viewWidth+'px';
            }
        }
        NTES('.ctrl_right a',o).addEvent('click',function(){
            playNext();
        });
        NTES('.ctrl_left a',o).addEvent('click',function(){
            playPrev();
        });
    }else{return false;}
}
NTES('.slide_places').each(function(i,item){
	infoZoneSlide(item);	
});
